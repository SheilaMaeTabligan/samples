class:

import capex.crypto

func main
{
	var bcrypt = BCryptEncoder.create()
	var salt = bcrypt.generateSalt(12)
	PRINT "Salt : " .. salt
	var hash = bcrypt.hashPassword("Super@dm1n", salt)
	PRINT "Hash Password : " .. hash
	var cp = bcrypt.checkPassword("Super@dm1n", hash)
	PRINT "Password is matched? : " .. String.asString(cp)
}
